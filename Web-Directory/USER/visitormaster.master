            DropDownList2.DataTextField = "Category_Name"
            DropDownList2.DataValueField = "Category_id"
            DropDownList2.DataBind()
            DropDownList2.Items.Insert(0, "Select Category")
            
            DropDownList3.DataSource = obj.calldata("select * from SubCategory_table")
            DropDownList3.DataTextField = "SubCategory_Name"
            DropDownList3.DataValueField = "SubCategory_id"
            DropDownList3.DataBind()
            DropDownList3.Items.Insert(0, "Select SubCategory")
        End If
        If Session("U_ID") Is Nothing Then
            LinkButton1.Text = "login"
            LinkButton2.Text = "login"
            LinkButton3.Text = "login"
            LinkButton4.Text = "login"
        Else
            LinkButton1.Text = "logout"
            LinkButton2.Text = "Go Advertise"
            LinkButton3.Text = "Go Advertise"
            LinkButton4.Text = "Go Advertise"
        End If
        
        ds = obj.calldata("select * from Scheme_table")
        Label2.Text = ds.Tables(0).Rows(0).Item(1).ToString()
        Label1.Text = ds.Tables(0).Rows(0).Item(3).ToString()
        Label3.Text = ds.Tables(0).Rows(0).Item(2).ToString()
        Label4.Text = ds.Tables(0).Rows(1).Item(1).ToString()
        Label5.Text = ds.Tables(0).Rows(1).Item(3).ToString()
        Label6.Text = ds.Tables(0).Rows(1).Item(2).ToString()
        Label7.Text = ds.Tables(0).Rows(2).Item(1).ToString()
        Label8.Text = ds.Tables(0).Rows(2).Item(3).ToString()
        Label9.Text = ds.Tables(0).Rows(2).Item(2).ToString()
        
    End Sub

