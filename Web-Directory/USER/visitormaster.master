        DropDownList3.DataValueField = "SubCategory_id"
        DropDownList3.DataBind()
        DropDownList3.Items.Insert(0, "Select Subcategory")
    End Sub
    
    Protected Sub Page_Load(ByVal sender As Object, ByVal e As System.EventArgs)
        
        If IsPostBack = False Then
            DropDownList1.DataSource = obj.calldata("select * from Area_table")
            DropDownList1.DataTextField = "Area_Name"
            DropDownList1.DataValueField = "Area_id"
            DropDownList1.DataBind()
            DropDownList1.Items.Insert(0, "Select Area")

